/* ---颜色(根据项目主题色)--- */
.text-black{
		color: #333333;
}
.text-white{
		color: #FFFFFF;
}
.text-gray{
		color: #888888;
}
.text-main-color{
		color: #FF6100;
}
.text-second-color{
		color: #FEA501;
}
.bg-main-color{
		background-color: #FF6100;
		color: #FFFFFF;
}
.bg-second-color{
		background-color: #FEA501;
		color: #FFFFFF;
}
.bg-gradual-main-second{
		background-image: -webkit-gradient(linear,left top, right top,from(#FF6100), to(#FEA501));
		background-image: -webkit-linear-gradient(left,#FF6100, #FEA501);
		background-image: linear-gradient(to right,#FF6100, #FEA501);
		color: #FFFFFF;
}
.bg-gradual-second-main{
		background-image: -webkit-gradient(linear, left top, right top, from(#FEA501),to(#FF6100));
		background-image: -webkit-linear-gradient(left, #FEA501,#FF6100);
		background-image: linear-gradient(to right, #FEA501,#FF6100);
		color: #FFFFFF;
}
